{
  "extends": "@tsconfig/strictest/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "noEmit": true,
    "skipLibCheck": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "typeRoots": [
      "node_modules/@types",
      "node_modules/@cloudflare/workers-types"
    ],
    "moduleResolution": "bundler"
  },
  "include": ["src/cf/index.ts"],
  "exclude": ["../socket.io"]
}
